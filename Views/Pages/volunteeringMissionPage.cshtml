                        <button class="bg-dark bg-opacity-50 border-0 card-btn" id="shareToFriend">
                            <img src="/images/user.png" alt="">
                        </button>
                        <div>
                            <p class="d-inline px-2" id="missionTheme">Environment</p>
                        </div>
                    </div>
